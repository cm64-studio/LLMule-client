{
  "name": "llmule-client",
  "version": "1.0.0",
  "description": "LLMule P2P Network Client - Share your LLMs with the world",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "enquirer": "^2.4.1",
    "express": "^4.21.2",
    "ora": "^5.4.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "llm",
    "ai",
    "p2p",
    "decentralized",
    "ollama",
    "lmstudio"
  ],
  "author": "Andy <andrescufari@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cm64-studio/LLMule-client.git"
  },
  "bugs": {
    "url": "https://github.com/cm64-studio/LLMule-client/issues"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
